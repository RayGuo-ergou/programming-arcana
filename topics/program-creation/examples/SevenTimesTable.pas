// 
// Program: SevenTimesTable.pas
// Displays the Seven Times Table from 1 x 7 to 10 x 7.
//
program SevenTimesTable;
begin
    WriteLn('Seven Times Table');
    WriteLn('-----------------');
    WriteLn(' 1 x 7 = ', 1 * 7);
    WriteLn(' 2 x 7 = ', 2 * 7);
    WriteLn(' 3 x 7 = ', 3 * 7);
    WriteLn(' 4 x 7 = ', 4 * 7);
    WriteLn(' 5 x 7 = ', 5 * 7);
    WriteLn(' 6 x 7 = ', 6 * 7);
    WriteLn(' 7 x 7 = ', 7 * 7);
    WriteLn(' 8 x 7 = ', 8 * 7);
    WriteLn(' 9 x 7 = ', 9 * 7);
    WriteLn('10 x 7 = ', 10 * 7);
    WriteLn('-----------------');
end.